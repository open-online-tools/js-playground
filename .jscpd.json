{
  "threshold": 0,
  "minTokens": 30,
  "minLines": 5,
  "skipComments": true,
  "ignore": [
    "**/node_modules/**",
    "**/build/**",
    "**/dist/**",
    "**/*.min.js",
    "**/coverage/**",
    "**/.changeset/**",
    "**/package-lock.json",
    "**/pnpm-lock.yaml",
    "**/yarn.lock"
  ],
  "format": "console",
  "reporters": ["console", "html"],
  "output": "./reports/jscpd"
}
